{
  "name": "android-init-language-syntax",
  "displayName": "Android Init Language Syntax",
  "description": "Syntax highlighting for Android init .rc files",
  "repository": {"url": "https://github.com/juliencombattelli/vscode-android-init-language-syntax"},
  "version": "0.1.0",
  "publisher": "juliencombattelli",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "rc",
        "aliases": [
          "Android Init RC File",
          "rc"
        ],
        "extensions": [
          ".rc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rc",
        "scopeName": "source.rc",
        "path": "./syntaxes/rc.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.inline.shellscript": "shellscript"
        }
      }
    ]
  }
}
